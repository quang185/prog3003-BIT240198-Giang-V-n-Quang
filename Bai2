public class Bai2 {

    public static <E> void printArray(E[] array) {
        for (E e : array) {
            System.out.println(e);
        }
    }

    public static void main(String[] args) {
        Integer[] a = {1, 2, 3};
        String[] b = {"A", "B", "C"};

        printArray(a);
        printArray(b);
    }
}
